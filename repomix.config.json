{
  "input": {
    "maxFileSize": 52428800
  },
  "output": {
    "filePath": "docs/repomix-output.md",
    "style": "markdown",
    "parsableStyle": true,
    "fileSummary": false,
    "directoryStructure": true,
    "files": true,
    "removeComments": false,
    "removeEmptyLines": false,
    // "compress": true,
    "compress": false,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": true,
    "git": {
      "sortByChanges": true,
      "sortByChangesMaxCommits": 100
    }
  },
  "include": [
    // ".relaycode/a85c5c0c-35cd-4ec1-a75d-16a760411ab6.yml"
    // "test/e2e/watch.test.ts",
    // "test/e2e/apply.test.ts",
    // "test/e2e/revert.test.ts"
    // "src/core/config.ts",
    // "src/core/parser.ts"
    // "src/commands/log.ts",
    // "src/commands/init.ts",
    // "src/commands/revert.ts",
    // "test/test.util.ts",
    // "test/e2e/init.test.ts",
    // "test/e2e/log.test.ts",
    // "test/e2e/revert.test.ts"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      ".relaycode",
      "fallbacks",
      ".cursor",
      "docs",
      "node_modules",
      ".gitignore",
      ".repomixignore",
      "bun.lock",
      "README.md",
      "repomix.config.json",
      "tsconfig.tsbuildinfo",
      "REQUIREMENT.md",
      "test"
      // "src/utils"
      // "test/e2e/patcher.test.ts",
      // "test/e2e/transaction.test.ts",
      // "test/e2e/watch.test.ts",
      // "test/e2e/init.test.ts",
      // "test/fixtures"
      // "test/unit",
      // "src/utils"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
